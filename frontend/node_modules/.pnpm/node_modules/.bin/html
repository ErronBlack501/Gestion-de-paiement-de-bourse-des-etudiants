#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/html@1.0.0/node_modules/html/bin/node_modules:/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/html@1.0.0/node_modules/html/node_modules:/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/html@1.0.0/node_modules:/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/html@1.0.0/node_modules/html/bin/node_modules:/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/html@1.0.0/node_modules/html/node_modules:/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/html@1.0.0/node_modules:/home/erb/Documents/ENI/L3/JSP/Gestion de paiement de bourse des étudiants/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html/bin/html.js" "$@"
else
  exec node  "$basedir/../html/bin/html.js" "$@"
fi
